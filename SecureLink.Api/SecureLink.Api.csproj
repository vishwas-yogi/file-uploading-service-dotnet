    <UserSecretsId>0cfd297e-87f6-47c8-b66a-3b03bfd9ae8e</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="10.0.0" />
